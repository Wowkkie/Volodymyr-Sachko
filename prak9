#include <stdio.h>
#include <stdlib.h>

int minSteps(int x, int y) {
    // Різниця між x та y
    int diff = y - x;

    // Якщо різниця менша або дорівнює 2, повертаємо різницю
    if (diff <= 2) {
        return diff;
    }

    // Ініціалізуємо змінні для трьох послідовних чисел
    int a = 1;
    int b = 2;
    int c = 3;

    // Поки наступне число (c) не перевищує різницю, обчислюємо наступне число
    while (c <= diff) {
        // Обчислюємо наступне число як суму двох попередніх
        c = a + b;
        // Переміщуємо попередні числа вперед
        a = b;
        b = c;
    }

    // Повертаємо кількість кроків, яка є різницею
    return c;
}

int main() {
    int x = 45;
    int y = 51;

    int steps = minSteps(x, y);

    printf("Мінімальна кількість кроків: %d\n", steps);

    return 0;
}
